<%= form_for @employee do |f| %>
    <%= f.label 'First Name: '%>
    <%= f.text_field :first_name %>
    <%= f.label 'Last Name: '%>
    <%= f.text_field :last_name %>
    <%= f.label 'Title: '%>
    <%= f.text_field :title %>
    <%= f.label 'Alias: '%>
    <%= f.text_field :alias %>
    <%= f.label 'Dog' %>
    <%= f.collection_select :dog_id, Dog.all, :id, :name %>
    <%= f.submit %>
<% end %>
    
